<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Logo (Design)"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <state
                jcr:primaryType="cq:Widget"
                fieldLabel="Search"
                name="./state"
                optionsProvider="function(path, record) { return CQ.Util.formatData(CQ.Util.eval('/apps/mywebsite/resources/json/states.json')); }"
                type="select"
                xtype="selection"/>
            <selection
                jcr:primaryType="cq:Widget"
                fieldLabel="Select sssss"
                name=".selection"
                options="/apps/services/myoptions.json"
                type="select"
                xtype="selection"/>
        </items>
    </items>
</jcr:root>
